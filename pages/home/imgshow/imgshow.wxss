/* pages/home/imgshow/imgshow.wxss */
.imglist{
  text-align: center;
}
.imglist{
  width: 700rpx;
  height: 415rpx;
  margin: 20rpx;
}

.imglist .imgitem{
  width: 100%;
}
.imglist .imgfull{
  width: 100%;
  height: 100px;
}
.imglist .imgitem{
  border-radius: 8px;
  box-shadow: 5px 8px 30px rgba(53,178,225,0.26);
}
.imglist .circle{
  width: 200px;
  height: 200px;
  border-radius: 100%;
}